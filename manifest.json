{
   "manifest_version": 3,
   "background": {
      "service_worker":  "service.js"
   },
   "chrome_url_overrides": {
      "newtab": "blank.html"
   },
   "description": "Redirect the New Tab page to your customize",
   "name": "FastUI New Tab",
   "permissions": ["tabs","storage"],
   "action": {
      "default_popup": "options.html",
      "default_icon": "logo.png"
   },
   "icons": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
   },
   "version": "1.0"
}
